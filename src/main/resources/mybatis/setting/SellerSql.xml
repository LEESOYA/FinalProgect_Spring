<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Seller">
	<!-- seller 회원가입 -->
	<insert id="insertOfSeller" parameterType="sdto">
		insert into seller(seller_company_number,seller_email,seller_pass) 
		values (#{seller_company_number},#{seller_email},#{seller_pass})
	</insert>
	
	<!-- seller 사업자 등록 -->
	<insert id="insertOfSellerCompany" parameterType="sdto">
		insert into seller_company 
		values (#{seller_company_number},#{seller_company_name},#{seller_company_image},#{seller_company_addr},
		        #{seller_company_type1},#{seller_company_type2},#{seller_company_hp},
		        #{seller_account_name},#{seller_account_holder},#{seller_account_number})
	</insert>
</mapper>